{
  "type": "extension",
  "name": "google_tts_python",
  "version": "0.1.2",
  "display_name": {
    "locales": {
      "en-US": {
        "content": "Google Text-to-Speech Extension"
      },
      "zh-CN": {
        "content": "Google TTS"
      }
    }
  },
  "description": {
    "locales": {
      "en-US": {
        "content": "Google Cloud Text-to-Speech extension for TEN Framework"
      },
      "zh-CN": {
        "content": "TEN Framework çš„ Google Cloud TTS"
      }
    }
  },
  "dependencies": [
    {
      "type": "system",
      "name": "ten_runtime_python",
      "version": "0.10"
    },
    {
      "type": "system",
      "name": "ten_ai_base",
      "version": "0.6"
    }
  ],
  "package": {
    "include": [
      "manifest.json",
      "property.json",
      "BUILD.gn",
      "**.tent",
      "**.py",
      "README.md",
      "requirements.txt"
    ]
  },
  "api": {
    "interface": [
      {
        "import_uri": "../../system/ten_ai_base/api/tts-interface.json"
      }
    ],
    "property": {
      "properties": {
        "params": {
          "type": "object",
          "properties": {
            "credentials": {
              "type": "string"
            },
            "language_code": {
              "type": "string"
            },
            "voice_name": {
              "type": "string"
            },
            "ssml_gender": {
              "type": "string"
            },
            "speaking_rate": {
              "type": "float64"
            },
            "pitch": {
              "type": "float64"
            },
            "volume_gain_db": {
              "type": "float64"
            },
            "audio_params": {
              "type": "object",
              "properties": {
                "sample_rate": {
                  "type": "int64"
                }
              }
            }
          }
        }
      }
    }
  }
}
